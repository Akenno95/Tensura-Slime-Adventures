<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	
		<!-- Human -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Tensura_Human_Gene</defName>
			<label>Human</label>
			<description>This pawn is considered to be a human.</description>
			<geneClass>EBSGFramework.HediffAdder</geneClass>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Tensura_Genes</displayCategory>
			<displayOrderInCategory>100</displayOrderInCategory>
			
			<modExtensions>
				<li Class="EBSGFramework.EBSGExtension">
					<hediffsToApply>
						<li>
							<hediff>Tensura_Race_Human</hediff>
							<severity>1</severity>
						</li>
					</hediffsToApply>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!--symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack>-->
		</GeneDef>
		
		
		<!-- Slime -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Tensura_Slime_Gene</defName>
			<label>Slime</label>
			<description>This pawn is considered to be a slime in human form.</description>
			<geneClass>EBSGFramework.HediffAdder</geneClass>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Tensura_Genes</displayCategory>
			<displayOrderInCategory>101</displayOrderInCategory>
			
			<modExtensions>
				<li Class="EBSGFramework.EBSGExtension">
					<hediffsToApply>
						<li>
							<hediff>Tensura_Race_Slime</hediff>
							<severity>1</severity>
						</li>
					</hediffsToApply>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!--symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack>-->
		</GeneDef>





</Defs>
