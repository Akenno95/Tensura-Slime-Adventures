			<label>Otherworlder</label>
			<description>This pawn is an Otherworlder.</description>
			<geneClass>EBSGFramework.HediffAdder</geneClass>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Tensura_Genes</displayCategory>
			<displayOrderInCategory>210</displayOrderInCategory>
			
			<modExtensions>
				<li Class="EBSGFramework.EBSGExtension">
					<hediffsToApply>
						<li>
							<hediff>Tensura_Otherworlder</hediff>
							<severity>3</severity>
						</li>
					</hediffsToApply>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!--symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack>-->
		</GeneDef>
		
		
