<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
		<!-- Human Xenotype -->
	
		<XenotypeDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Tensura_Human</defName>
			<label>Human</label>
			<description>This pawn is considered to be a human.</description>
			<iconPath>UI/Icons/Xenotypes/Baseliner</iconPath>
			<genes>
				<li>Tensura_Human_Gene</li>
			</genes>
		</XenotypeDef>
		
		
		<!-- Slime Xenotype -->
	
		<XenotypeDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Tensura_Slime</defName>
			<label>Slime</label>
			<description>This pawn is considered to be a slime in human form.</description>
			<iconPath>UI/Icons/Xenotypes/Baseliner</iconPath>
			<genes>
				<li>Tensura_Slime_Gene</li>
			</genes>
		</XenotypeDef>


</Defs>
