			<ItsSorceryFramework.LearningTreeNodeDef ParentName = "LearningTreeNodeBase">
				<learningTrackerDef>Tensura_Nullifications_LearningTrackerTree</learningTrackerDef>
				<defName>Tensura_Node_PhysicalNullification</defName>
				<label>Physical Damage Nullification</label>
				<description>You can nullify physical damage.</description>
				
				<coordX>0</coordX>
				<coordY>0</coordY>
				<pointReq>10</pointReq>
				
				<prereqs>
					<li>Isekai_Resistances_Node_BluntResistance5</li>
					<li>Isekai_Resistances_Node_SharpResistance5</li>
				</prereqs>
				
				<statOffsets>
					<ArmorRating_Blunt>10.0</ArmorRating_Blunt>
					<ArmorRating_Sharp>10.0</ArmorRating_Sharp>
				</statOffsets>
			</ItsSorceryFramework.LearningTreeNodeDef>


